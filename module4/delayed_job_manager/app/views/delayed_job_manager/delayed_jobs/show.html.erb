<div class="container">
  <h1>Delayed Job</h1>

  <table class="table">
    <tr>
      <th>ID</th>
      <td><%= @delayed_job.id %></td>
    </tr>
    <tr>
      <th>Priority</th>
      <td><%= @delayed_job.priority %></td>
    </tr>
    <tr>
      <th>Attempts</th>
      <td><%= @delayed_job.attempts %></td>
    </tr>
    <tr>
      <th>Handler</th>
      <td><%= @delayed_job.handler %></td>
    </tr>
    <tr>
      <th>Last Error</th>
      <td><%= @delayed_job.last_error %></td>
    </tr>
    <tr>
      <th>Run At</th>
      <td><%= @delayed_job.run_at %></td>
    </tr>
    <tr>
      <th>Locked At</th>
      <td><%= @delayed_job.locked_at %></td>
    </tr>
    <tr>
      <th>Failed At</th>
      <td><%= @delayed_job.failed_at %></td>
    </tr>
    <tr>
      <th>Locked By</th>
      <td><%= @delayed_job.locked_by %></td>
    </tr>
    <tr>
      <th>Queue</th>
      <td><%= @delayed_job.queue %></td>
    </tr>
    <tr>
      <th>Created At</th>
      <td><%= @delayed_job.created_at %></td>
    </tr>
    <tr>
      <th>Updated At</th>
      <td><%= @delayed_job.updated_at %></td>
    </tr>
  </table>

  <%= link_to 'Edit', edit_delayed_job_path(@delayed_job), class: 'btn btn-primary' %> |
  <%= link_to 'Back', delayed_jobs_path, class: 'btn btn-secondary' %>
</div>