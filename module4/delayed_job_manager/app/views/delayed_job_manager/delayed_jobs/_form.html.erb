<%= form_with(model: delayed_job, local: true) do |form| %>
  <% if delayed_job.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(delayed_job.errors.count, "error") %> prohibited this delayed_job from being saved:</h2>

      <ul>
        <% delayed_job.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= form.label :priority %>
    <%= form.number_field :priority, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= form.label :attempts %>
    <%= form.number_field :attempts, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= form.label :handler %>
    <%= form.text_area :handler, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= form.label :last_error %>
    <%= form.text_area :last_error, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= form.label :run_at %>
    <%= form.datetime_select :run_at, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= form.label :locked_at %>
    <%= form.datetime_select :locked_at, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= form.label :failed_at %>
    <%= form.datetime_select :failed_at, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= form.label :locked_by %>
    <%= form.text_field :locked_by, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= form.label :queue %>
    <%= form.text_field :queue, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= form.submit class: 'btn btn-primary' %>
  </div>
<% end %>