<div class="container">
  <h1>Delayed Jobs</h1>
  <table class="table">
    <thead>
    <tr>
      <th>ID</th>
      <th>Priority</th>
      <th>Attempts</th>
      <th>Handler</th>
      <th>Last Error</th>
      <th>Run At</th>
      <th>Locked At</th>
      <th>Failed At</th>
      <th>Locked By</th>
      <th>Queue</th>
      <th>Created At</th>
      <th>Updated At</th>
      <th colspan="3"></th>
    </tr>
    </thead>
    <tbody>
    <% @delayed_jobs.each do |delayed_job| %>
      <tr>
        <td><%= delayed_job.id %></td>
        <td><%= delayed_job.priority %></td>
        <td><%= delayed_job.attempts %></td>
        <td><%= delayed_job.handler %></td>
        <td><%= delayed_job.last_error %></td>
        <td><%= delayed_job.run_at %></td>
        <td><%= delayed_job.locked_at %></td>
        <td><%= delayed_job.failed_at %></td>
        <td><%= delayed_job.locked_by %></td>
        <td><%= delayed_job.queue %></td>
        <td><%= delayed_job.created_at %></td>
        <td><%= delayed_job.updated_at %></td>
        <td><%= link_to 'Show', delayed_job %></td>
        <td><%= link_to 'Edit', edit_delayed_job_path(delayed_job) %></td>
        <td><%= link_to 'Destroy', delayed_job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
  <%= link_to 'New Delayed Job', new_delayed_job_path, class: 'btn btn-primary' %>
</div>